{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^firebase/",
    "^react-native$",
    "^@/(?!shared/)",
    "^@tamagui/",
    "^tamagui$",
    "^@/shared/models/",
    "^(?!yup$)\\w[\\w-]*\\w$",
    "^yup$",
    "@hookform/resolvers/yup",
    "^@/shared/components",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
